<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <Version>2.2.2</Version>
    <Authors>Rik Essenius</Authors>
    <Company />
    <Description>Sample .Net Standard fixture for FitNesse</Description>
    <Copyright>Copyright Â© 2015-2020 Rik Essenius</Copyright>
    <PackageLicenseUrl>https://github.com/essenius/FitNesseFitSharpFeatureDemos/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/essenius/FitNesseFitSharpFeatureDemos</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702</NoWarn>
